@model List<Category>

<h1>Category List</h1>

<table class="table table-bordered table-striped my-3">
	<thead>
	<tr>
		<th>Id</th>
		<th>Category</th>
		<th>Display Order</th>
	</tr>
	</thead>
	<tbody>
		@foreach (var obj in Model.OrderBy(u=>u.DisplayOrder))
		{
			<tr>
				<td>@obj.Id</td>
				<td>@obj.Name</td>
				<td>@obj.DisplayOrder</td>
			</tr>
		}
	</tbody>
</table>